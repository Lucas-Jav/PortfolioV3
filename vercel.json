{
  "version": 2,
  "builds": [
    { "src": "api/*.js", "use": "@vercel/node" },
    { "src": "frontend/package.json", "use": "@vercel/static-build" }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/api/.*", "dest": "api/index.js" },
    { "src": "/(.*)", "dest": "frontend/index.html" }
  ]
}
